<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wqc.mapper.UsersMapper">
	<insert id="insertUser" parameterType="users">
		insert into users(name,age) values(#{name},#{age})
	</insert>
	<select id="selectUserAll" resultType="users">
		select id,name,age from users
	</select>
	<select id="selectUsersById" resultType="users">
		select id,name,age from users where id = #{id}
	</select>
	<update id="updataUser" parameterType="users">
		update users set name=#{name}, age=#{age} where id=#{id}
	</update>
	<delete id="deleteUserById" >
		delete from users where id=#{id}
	</delete>
	<insert id="insertAdms" parameterType="users">
		insert into adms(name,password) values(#{name},#{password})
	</insert>
	
</mapper>